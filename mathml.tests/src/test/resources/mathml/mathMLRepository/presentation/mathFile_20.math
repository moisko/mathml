<?xml version="1.0" encoding="UTF-8"?>
<mml:math xmlns:mml="http://www.w3.org/1998/Math/MathML" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xsi:schemaLocation="http://www.w3.org/1998/Math/MathML http://www.w3.org/Math/XMLSchema/mathml2/mathml2.xsd">
	<mml:mrow>
		<mml:msup>
			<mml:mrow>
				<mml:mi>cos</mml:mi>  
				<mml:mo>&#x2061;</mml:mo> 
				<mml:mfenced>	
					<mml:mrow>
						<mml:mi>sin</mml:mi> 
						<mml:mo>&#x2061;</mml:mo>
						<mml:mfenced>
							<mml:mn>30</mml:mn>
						</mml:mfenced>	
					</mml:mrow>
				</mml:mfenced>
			</mml:mrow>
			<mml:mrow>
				<mml:mn>1</mml:mn>
				<mml:mo>+</mml:mo> 
				<mml:msup> 
					<mml:mn>1</mml:mn>
					<mml:mn>3</mml:mn>
				</mml:msup>
			</mml:mrow>
		</mml:msup>	   
		<mml:mo>+</mml:mo>
		<mml:mfrac>	 
			<mml:mrow>			  
				<mml:mn>1</mml:mn>
				<mml:mo>+</mml:mo>
				<mml:mn>3</mml:mn>
			</mml:mrow>
			<mml:mn>4</mml:mn>	
		</mml:mfrac>	 
	</mml:mrow>
</mml:math>
